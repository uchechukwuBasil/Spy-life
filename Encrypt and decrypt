#include <iostream>
#include <string>
using namespace std;

int main ()
{
string s;  int x=1; string n;  string c;
getline(cin,s);  
int j= s.length();
for(int i=0; i <= j; i++){
n +=s[j-x];
x++;
}  //cout<<n<<endl;
int len = n.length();
for(int i=0;i<len; i++)
{
if ((n[i]==32)||(n[i]>=65 && n[i]<=90)||(n[i]>=97 && n[i]<=122))
{
c += n[i];
}
else {
   //
}}
 cout<<c<<endl;
return 0;
}
